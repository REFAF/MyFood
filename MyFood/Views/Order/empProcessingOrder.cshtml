@model IEnumerable<MyFood.Models.FoodReceiptForm3>
@{
    ViewBag.Title = "empProcessingOrder";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<br />
<br />
<h3 class="font-KhebratMusamim green">طلبات قيد التنفيذ</h3>
<br />

<table class="table">
    <tr class="green-label">
        <th>
            @Html.DisplayName("رقم الطلب")
        </th>
    </tr>


    @foreach (var item in Model)
    {
        <tr class="gold-label">
            <td>
                @Html.DisplayFor(modelItem => item.Order.order_id)
            </td>
            <td>
                @Html.ActionLink("عرض الطلب", "Form3Sec2",null,
               new { id = item.f3_id }, new { @class = "gold-link" })
            </td>
        </tr>
    }

</table>